# Security: This file should have restricted permissions (chmod 600).
# The bot sets this automatically at startup.
TOKEN_RECEIVE_ADDRESS=""
# FUNDING_WIF is stored in config/wallets.json (encrypted).
# Run: yarn manage â†’ "Encrypt wallets file" to migrate.
# Legacy: set FUNDING_WIF="" here if not using encrypted wallets.json.
API_KEY=""
RATE_LIMIT=32
DEFAULT_OUTBID_MARGIN=0.00000001
DEFAULT_COUNTER_BID_LOOP_TIME=30
DEFAULT_LOOP=30

# Multi-Wallet Rotation
# Rotates between funding wallets so bids come from different addresses.
# Each wallet is rate-limited to ~5 bids/min (Magic Eden per-wallet limit).
# Total throughput scales with wallet count (2 wallets = 10 bids/min).
ENABLE_WALLET_ROTATION=false
WALLET_CONFIG_PATH=./config/wallets.json

# Bid Pacing (adaptive rate limiting)
# Magic Eden limits wallets to ~5 bids per minute
# The pacer tracks bids in a 60-second window and waits when limit is reached
BIDS_PER_MINUTE=5

# Set to false to allow overlapping bid cycles (not recommended)
# When true, if a scheduled cycle is still running when the next one starts, it will be skipped
SKIP_OVERLAPPING_CYCLES=true